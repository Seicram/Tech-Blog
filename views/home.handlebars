<!-- home.handlebars -->

<!DOCTYPE html>
<html>
<head>
  <title>My Blog</title>
  <!-- Add CSS and other head elements -->
</head>
<body>
  <header>
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/dashboard">Dashboard</a></li>
        <li><a href="/logout">Log Out</a></li>
      </ul>
    </nav>
  </header>

  <main>
    <h1>Homepage</h1>
    {{#if posts}}
      <ul>
        {{#each posts}}
          <li>
            <h2>{{title}}</h2>
            <p>Created at: {{createdAt}}</p>
          </li>
        {{/each}}
      </ul>
    {{else}}
      <p>No posts found.</p>
    {{/if}}
  </main>

  <!-- Add JavaScript and other script tags -->
</body>
</html>
